Github-Patches: .github/workflows/haskell-ci.patch
Copy-Fields: some

Distribution: jammy

Hlint: True
Hlint-Job: 9.2.4
Hlint-Version: ^>=3.4

Local-Ghc-Options:
  -Werror
  -optc=-Werror

Constraint-Set unix-2.7
  Constraints: unix ^>=2.7
  Tests:       True
  Run-Tests:   True

Constraint-Set unix-2.8
  Constraints: unix ^>=2.8
  Tests:       True
  Run-Tests:   True
